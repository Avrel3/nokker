{
  "name": "nokker",
  "version": "1.0.0",
  "description": "Nodejs cli for building projects",
  "private": false,
  "license": "MIT",
  "main": "lib",
  "files": [
    "lib"
  ],
  "bin": {
    "nokker": "bin/nokker",
    "nkr": "bin/nkr"
  },
  "homepage": "https://github.com/Avrel3/nokker/#readme",
  "scripts": {
    "build": "tsc",
    "build:esm": "tsc --module es6 --target es5 --outDir lib/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib/cjs",
    "export": "npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "cli",
	"git",
	"license generator",
	"config generator",
	"template generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Avrel3/nokker.git"
  },
  "author": {
    "name": "Avrel",
    "url": "https://github.com/Avrel3"
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "axios": "^1.1.3",
    "colorette": "^2.0.19",
    "enquirer": "^2.3.6",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^18.11.9",
    "@types/yargs": "^17.0.13"
  }
}
